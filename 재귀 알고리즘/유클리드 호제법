package Recursion;

public class Euclid {

    public static int Euclid(int x, int y){
        if(y==0){
            return  x;
        }
        else {
            return Euclid(y, x % y);
        }
    }
    public static void main(String[] args) {

        System.out.println(Euclid(21,14));
    }
}

